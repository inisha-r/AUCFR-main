{
  "version": 2,
  "builds": [
    {
      "src": "app.py",  
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/hello",
      "dest": "/app.py"  
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"  
  },
  "build": {
    "env": {
      "BUILD_SCRIPT": "./install.sh"
    }
  }
}
